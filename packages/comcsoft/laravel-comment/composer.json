{
    "name": "comcsoft/laravel-comment",
    "description": "A laravel package for commenting.",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/auth": "^5.4",
        "kalnoy/nestedset": "^4.3",
        "spatie/laravel-medialibrary": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "欧军辉",
            "email": "oujh@91360.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Comcsoft\\Comment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Comcsoft\\Comment\\CommentServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
